{
    "name": "descom/image-x",
    "description": "Image manipulation to web",
    "type": "library",
    "require": {
        "php": "^8.2",
        "intervention/image": "^2.7"
    },
    "homepage": "https://github.com/descom-es/image-x",
    "license": "MIT",
    "authors": [
        {
            "name": "Cesar Garcia",
            "email": "cesar@descom.es",
            "role": "Developer"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpunit/phpunit": "^9.3|^10.0|^10.5",
        "phpstan/phpstan": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Descom\\ImageX\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Descom\\ImageX\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "style": "vendor/bin/php-cs-fixer fix",
        "analyse": "vendor/bin/phpstan analyse"
    }
}
